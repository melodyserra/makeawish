<% include ../partials/header %>

<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="inputsummary" class="col-sm-2 control-label">Summary</label>
    <div class="col-sm-10">
      <!-- <input type="text" class="form-control" id="summary"> -->
      <textarea class="form-control" id="summary" cols="30" rows="5"></textarea>
    </div>
  </div>
  <div class="form-group">
    <label for="inputname" class="col-sm-2 control-label">Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="name">
    </div>
  </div>
  <div class="form-group">
    <label for="inputage" class="col-sm-2 control-label">Age</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="age">
    </div>
  </div>
  <div class="form-group">
    <label for="inputillness" class="col-sm-2 control-label">Illness</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="illness">
    </div>
  </div>
  <div class="form-group">
    <label for="inputwish" class="col-sm-2 control-label">Wish</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="wish">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div>
</form>

<% include ../partials/footer %>